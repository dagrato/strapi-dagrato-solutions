{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "string"
    },
    "published": {
      "type": "date"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "content.paragraph",
        "code-block.code-block",
        "image-block.image-block"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cat.cat",
      "inversedBy": "articles"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "articles"
    },
    "views": {
      "type": "integer",
      "default": 0
    }
  }
}
